{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
		"include": [ "DuAEF.jsxinc" ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "copyright"             : "Copyright (c) 2017-2019 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "theme"                 : "slate",
        "collapseSymbols"       : "false",
        "syntaxTheme"           : "dark",
        "search"                : "true"
    },
	"opts": {
        "template": "../src-docs/templates/node_modules/tui-jsdoc-template/",
		"recurse": false,
        "destination": "../docs/Reference"
    }
}
